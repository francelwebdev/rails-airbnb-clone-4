<div class="wrapper-grey padded">
  <div class="heading text-center">
  <h2>Your rentals:</h2>
  </div>
  <div class="container">
    <div class="row">
      <% current_user.rentals.each do |r| %>
      <div class="col-xs-6 col-xs-offset-3">
        <div class="devise">
          <li><%= r.equipment.title %></li>
          <li><%= r.equipment.description %></li>
          <li>Rent from: <%= r.start_date %></li>
          <li>Rent to: <%= r.end_date %></li>
        </div>
      </div>
      <% end %>
    </div>
  </div>
</div>




